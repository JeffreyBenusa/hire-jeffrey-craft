{
  "name": "jeffrey-benusa/craft-hire-jeffrey-chat",
  "type": "craft-plugin",
  "require": {
    "php": ">=8.2",
    "ext-dom": "*",
    "ext-libxml": "*",
    "craftcms/cms": "^5.6.0",
    "openai-php/client": "^0.10.3"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "jeffreybenusa\\crafthirejeffreychat\\": "src/"
    }
  },
  "extra": {
    "handle": "_hire-jeffrey-chat",
    "name": "hire-jeffrey-chat",
    "developer": "Jeffrey Benusa",
    "documentationUrl": ""
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "php-http/discovery": true,
      "yiisoft/yii2-composer": true
    }
  }
}
